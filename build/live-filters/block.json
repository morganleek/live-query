{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "scm/live-filters",
  "version": "1.0.0",
  "title": "Live Filters",
  "category": "widgets",
  "icon": "arrow-down-alt2",
  "description": "",
  "example": {},
  "supports": {
    "html": false,
    "spacing": {
      "blockGap": [
        "horizontal",
        "vertical"
      ]
    }
  },
  "ancestor": [
    "scm/live-query"
  ],
  "attributes": {
    "filters": {
      "type": "array",
      "default": []
    },
    "filterLabels": {
      "type": "object",
      "default": {}
    },
    "multiSelect": {
      "type": "boolean",
      "default": true
    },
    "hideEmpty": {
      "type": "boolean",
      "default": false
    },
    "layout": {
      "type": "string",
      "default": "select"
    }
  },
  "textdomain": "live-filters",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "render": "file:./index.php"
}